<%= form_for([menu, food], html: { multipart: true }) do |f| %>
  <div class="form-group">
  	<%= f.label :name %><br>
  	<%= f.text_field :name %>
  </div>
  <div class="form-group">
  	<%= f.label :price %><br>
  	<%= f.text_field :price %>
  </div>
  <div class="form-group">
  	<%= f.label :image, "Food Image" %><br>
  	<%= f.file_field :image, class: "form-control" %>
  </div><br>
  <%= f.submit %>
<% end %>